{
    "mappings": {
        "dynamic_templates": [
            {
                "string": {
                    "mapping": {
                        "store": true,
                        "type": "keyword"
                    },
                    "match": "*_s",
                    "match_mapping_type": "string"
                }
            },
            {
                "text": {
                    "mapping": {
                        "analyzer": "gc_english",
                        "store": true,
                        "type": "text"
                    },
                    "match": "*_t",
                    "match_mapping_type": "string"
                }
            },
            {
                "string_text": {
                    "mapping": {
                        "fields": {
                            "search": {
                                "normalizer": "lowercase_normalizer",
                                "type": "keyword"
                            }
                        },
                        "store": true,
                        "type": "keyword"
                    },
                    "match": "*_ks",
                    "match_mapping_type": "string"
                }
            },
            {
                "integer": {
                    "mapping": {
                        "store": true,
                        "type": "integer"
                    },
                    "match": "*_i",
                    "match_mapping_type": "long"
                }
            },
            {
                "long": {
                    "mapping": {
                        "store": true,
                        "type": "long"
                    },
                    "match": "*_l",
                    "match_mapping_type": "long"
                }
            },
            {
                "boolean": {
                    "mapping": {
                        "store": true,
                        "type": "boolean"
                    },
                    "match": "*_b",
                    "match_mapping_type": "boolean"
                }
            },
            {
                "double": {
                    "mapping": {
                        "store": true,
                        "type": "double"
                    },
                    "match": "*_d",
                    "match_mapping_type": "double"
                }
            },
            {
                "float": {
                    "mapping": {
                        "store": true,
                        "type": "float"
                    },
                    "match": "*_f",
                    "match_mapping_type": "double"
                }
            },
            {
                "date": {
                    "mapping": {
                        "format": "yyyy-MM-dd\u0027T\u0027HH:mm:ss",
                        "store": true,
                        "type": "date"
                    },
                    "match": "*_dt"
                }
            },
            {
                "rank_feature": {
                    "mapping": {
                        "store": true,
                        "type": "rank_feature"
                    },
                    "match": "*_r"
                }
            },
            {
                "rank_features": {
                    "mapping": {
                        "store": true,
                        "type": "rank_features"
                    },
                    "match": "*_rs"
                }
            },
            {
                "nested_object": {
                    "mapping": {
                        "type": "nested"
                    },
                    "match": "*_n"
                }
            },
            {
                "default_type": {
                    "mapping": {
                        "fields": {
                            "keyword": {
                                "fields": {
                                    "search": {
                                        "ignore_above": 256,
                                        "normalizer": "lowercase_normalizer",
                                        "type": "keyword"
                                    }
                                },
                                "ignore_above": 256,
                                "store": true,
                                "type": "keyword"
                            }
                        },
                        "store": true,
                        "type": "text"
                    },
                    "path_match": "*"
                }
            }
        ],
        "properties": {
            "author": {
                "store": true,
                "type": "keyword"
            },
