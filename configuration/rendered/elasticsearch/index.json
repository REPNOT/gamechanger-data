            "pagerank": {
                "type": "rank_feature"
            },
            "paragraphs": {
                "dynamic": "true",
                "properties": {
                    "entities": {
                        "dynamic": "true",
                        "type": "nested"
                    },
                    "filename": {
                        "fields": {
                            "search": {
                                "normalizer": "lowercase_normalizer",
                                "type": "keyword"
                            }
                        },
                        "store": true,
                        "type": "keyword"
                    },
                    "id": {
                        "store": true,
                        "type": "keyword"
                    },
                    "par_inc_count": {
                        "store": true,
                        "type": "long"
                    },
                    "par_raw_text_t": {
                        "analyzer": "standard",
                        "fields": {
                            "gc_english": {
                                "analyzer": "gc_english",
                                "type": "text"
                            }
                        },
                        "store": true,
                        "type": "text"
                    },
                    "type": {
                        "store": true,
                        "type": "keyword"
                    }
                },
                "type": "nested"
            },
            "pop_score": {
                "store": true,
                "type": "long"
            },
            "ref_list": {
                "store": true,
                "type": "keyword"
            },
            "signature": {
                "store": true,
                "type": "keyword"
            },
            "subject": {
                "store": true,
                "type": "keyword"
            },
            "summary_30": {
